{
  "manifest_version": 2,

  "name": "Snaskblock",
  "description": "Håller dina fingrar borta från snasket…",
  "version": "1.0.0",
  "icons": {
    "64": "images/icon.png"
  },
  "web_accessible_resources": [
    "audio/warning.wav"
  ],
  "permissions": [
     "*://www.mathem.se/*",
     "*://www.coop.se/*",
     "*://www.mat.se/*"
   ],
   "content_scripts": [
      {
       "matches": [
         "*://www.mathem.se/*",
         "*://www.coop.se/*",
         "*://www.mat.se/*"
       ],
       "js": [
         "js/jquery.min.js",
         "js/snaskblock.js"
       ],
       "css": [
         "css/snaskblock.css"
       ]
     },
     {
       "matches": [
         "*://www.mathem.se/*"
       ],
       "js": [
         "js/mathem.js"
       ]
     },
     {
       "matches": [
         "*://www.coop.se/*"
       ],
       "js": [
         "js/coop.js"
       ]
     },
     {
       "matches": [
         "*://www.mat.se/*"
       ],
       "js": [
         "js/mat.js"
       ]
     }
   ]
}
